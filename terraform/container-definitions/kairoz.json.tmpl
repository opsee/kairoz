[
    {
        "Name": "kairoz",
        "Image": "quay.io/opsee/cats:${version}",
        "Memory": 256,
        "Essential": true,
        "Command": ["/run.sh"],
        "Environment": [
            { "Name": "APPENV", "Value": "${appenv}" }
        ],
        "PortMappings": [{
            "hostPort": 9113,
            "containerPort": 9114,
            "protocol": "tcp"
        }],
        "LogConfiguration": {
            "LogDriver": "syslog",
            "Options": {
                "syslog-address": "${syslog_address}",
                "tag": "${syslog_tag}",
                "syslog-tls-skip-verify": "true"
            }
        }
    }
]
